{
  "serv":{
    "port":5000,
    "baseDir":"/home/local/USHERBROOKE/ranj2004/Téléchargements/CoLab/",
    "disableAuth":true,
    "debug":true,
    "log":true,
    "delTokensOnShutdown":false,
    "streamer":{
      "mode": "development",
      "maxAge": "3600",
      "customMimes":{
        ".web-mp4":"video/mp4"
      }
    }
  },
  "auth_handlers":[
    "mongo"
  ],
  "applications":{
    "colab":{
      "auth_handler":{
        "name":"mongo",
        "uri" :"mongodb://localhost:27017",
        "options":{},
        "reconnectAfter_s":3600
      },
      "projects":["DEI"]
    },
    "myapp":{
      "auth_handler":null,
      "projects":["Son"]
    }
  },
  "redis":{
    "socket":"/tmp/redis-media-node.sock",
    "expire_min":1440
  },
  "projects":{
    "DEI":{
      "rootDir":"DEI",
      "expRegex":"*",
      "mediaDir":"video",
      "mediaRegex":"*.web-mp4"
    },
    "Son":{
      "rootDir":"Son",
      "expRegex":"*",
      "mediaDir":null,
      "mediaRegex":"*.wav"
    }
  }
}
